        "foreground": "#656565", // used by Settings panel
        "textBlockQuote.background": "#c8c8c8", // can be seen in Interactive Playground
        // "textBlockQuote.border": "#00fff0",
        "textCodeBlock.background": "#c8c8c8",
        "textLink.activeForeground": "#f000ff",
        "textLink.foreground": "#0000ff",
        "textPreformat.foreground": "#650000",
